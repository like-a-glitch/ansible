[
    {
        "command": "editor.action.deleteLines",
        "key": "shift+delete",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "command": "editor.action.duplicateSelection",
        "key": "ctrl+d"
    },
    {
        "command": "editor.action.sortLinesAscending",
        "key": "ctrl+m ctrl+f9"
    },
    {
        "command": "workbench.action.files.saveFiles",
        "key": "ctrl+shift+s"
    }
]